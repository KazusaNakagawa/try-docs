{
  "version": 1,
  "formatters": {
    "defaultFormatter": {
      "format": "%(asctime)s %(name)-15s %(levelname)-8s %(message)sã€€%(process)d",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "userFormatter": {
      "format": "%(asctime)s %(name)-15s %(levelname)-8s user_id: %(user_id)-5s %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "articleFormatter": {
      "format": "%(asctime)s %(name)-15s %(levelname)-8s article_id: %(article_id)-5s %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "defaultHandlers": {
      "class": "logging.StreamHandler",
      "formatter": "defaultFormatter",
      "level": "DEBUG"
    },
    "userHandlers": {
      "class": "logging.StreamHandler",
      "formatter": "userFormatter",
      "level": "DEBUG"
    },
    "articleHandlers": {
      "class": "logging.StreamHandler",
      "formatter": "articleFormatter",
      "level": "DEBUG"
    }
  },
  "root": {
    "handlers": [
      "defaultHandlers"
    ],
    "level": "WARNING"
  },
  "loggers": {
    "simpleDefault": {
      "handlers": [
        "defaultHandlers"
      ],
      "level": "DEBUG",
      "propagate": 0
    },
    "simpleUser": {
      "handlers": [
        "userHandlers"
      ],
      "level": "DEBUG",
      "propagate": 0
    },
    "simpleArticle": {
      "handlers": [
        "articleHandlers"
      ],
      "level": "DEBUG",
      "propagate": 0
    }
  }
}