{
  "version": 1,
  "formatters": {
    "sampleFormatter": {
      "format": "%(asctime)s %(name)-12s %(levelname)-8s %(name)-15s %(message)s",
      "datefmt": "Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "sampleHandlers": {
      "class": "logging.StreamHandler",
      "formatter": "sampleFormatter",
      "level": "DEBUG"
    }
  },
  "root": {
    "handlers": [
      "sampleHandlers"
    ],
    "level": "WARNING"
  },
  "loggers": {
    "simpleExample": {
      "handlers": [
        "sampleHandlers"
      ],
      "level": "DEBUG",
      "propagate": 0
    },
    "simpleExample2": {
      "handlers": [
        "sampleHandlers"
      ],
      "level": "CRITICAL",
      "propagate": 0
    }
  }
}